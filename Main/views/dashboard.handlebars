<div class="modal fade" id="projectModal" tabindex="-1" aria-labelledby="projectModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="projectModalLabel">Create Project</h1>
      </div>
      <div class="modal-body">
        <form class="row g-3" id="projectForm">
        <div class="col-12">
            <label for="project_name" class="form-label" id="project_name_label">Project Name</label>
            <div class="input-group">
              <input type="text" class="form-control" id="project_name" placeholder="Train Employees" required>
            </div>
        </div>
        <div class="col-12">
            <label for="description" class="form-label" id="description_label">Project Description</label>
            <div class="input-group">
              <textarea class="form-control" id="description" placeholder="Onboard employees with new technology" required></textarea>
            </div>
        </div>
        <div class="col-12">
            <label for="due_date" class="form-label" id="due_date_label">Project Due Date</label>
            <div class="input-group">
              <input type="text" class="form-control" id="due_date" placeholder="1/1/2023" required>
            </div>
        </div>
        <div class="col-12">
          <label for="employeeDataList" class="form-label" id="div_id_label">Associated Division</label>
          <input class="form-control" list="datalistOptions" id="div_id" placeholder="Type to search...">
          <datalist id="datalistOptions">
            {{#each division}}
            <option value={{id}}>
            {{/each}}
          </datalist>
        </div>
      <div class="col-12">
        <button id="createProject" class="btn btn-primary" type="submit">Create</button>
        <button type="button" class="btn bg-gradient-secondary" data-bs-dismiss="modal">Cancel</button>
      </div>
      </form>
      </div>
    </div>
  </div>
</div>
