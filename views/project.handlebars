<div class="modal fade" id="settingsModal" tabindex="-1" aria-labelledby="settingsModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="settingsModalLabel">Settings</h1>
      </div>
      <div class="modal-body">
        <form class="row g-3" id="settingsForm">
        <div class="col-12">
            <label for="new_password" class="form-label">New Password</label>
        <div class="input-group">
            <input type="password" class="form-control" id="new_password" placeholder="â€¢â€¢â€¢â€¢â€¢â€¢" required>
        </div>
      </div>
      <div class="col-12">
            <label for="reenter_password" class="form-label">Reenter Password</label>
        <div class="input-group">
            <input type="password" class="form-control" id="reenter_password" placeholder="â€¢â€¢â€¢â€¢â€¢â€¢" required>
        </div>
      </div>
      <div class="col-12">
        <button id="changePassword" class="btn btn-primary" type="submit">Change Password</button>
        <button type="button" class="btn bg-gradient-secondary" data-bs-dismiss="modal">Cancel</button>
      </div>
      </form>
      </div>
    </div>
  </div>
</div>


